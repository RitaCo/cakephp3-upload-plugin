{
    "name": "ritaco/upload",
    "description": "RitaUpload is plugin for upload files",
    "keywords": ["cakephp", "plugin", "upload", "files"],
    "type": "cakephp-plugin",
    "homepage": "https://github.com/RitaCo/cakephp3-uplod-plugin",
    "license": "MIT",
    "version": "0.0.1",
    "authors": [
        {
            "name": "Saleh Souzanchi",
            "role": "Author",
            "homepage": "http://www.ritaco.net"
        }
    ],
    "require": {
        "php": ">=5.4",
        "cakephp/plugin-installer": "*",
        "cakephp/cakephp": "3.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "RitaUpload\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RitaUpload\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    },
    "support":{
        "source": "https://github.com/RitaCo/cakephp3-upload-plugin",
        "issues": "https://github.com/RitaCo/cakephp3-upload-plugin/issues"
    },
    "extra": {
        "installer-name": "RitaUpload"
    }    
    
}
